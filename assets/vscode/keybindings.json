// 将键绑定放在此文件中以覆盖默认值
[
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inProjectManagerList"
  },
  {
    "key": "ctrl+shift+alt+p",
    "command": "projectManager.listProjectsNewWindow"
  },
  {
    "key": "ctrl+k ctrl+shift+oem_4",
    "command": "extension.partialDiff.markSection1",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k ctrl+shift+oem_6",
    "command": "extension.partialDiff.markSection2AndTakeDiff",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k ctrl+shift+oem_minus",
    "command": "extension.partialDiff.diffSelectionWithClipboard",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k ctrl+shift+oem_plus",
    "command": "extension.partialDiff.diffVisibleEditors",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+k ctrl+oem_plus",
    "command": "wwm.aligncode"
  },
  {
    "key": "ctrl+k ctrl+shift+u",
    "command": "editor.action.transformToUppercase"
  },
  {
    "key": "ctrl+k ctrl+shift+l",
    "command": "editor.action.transformToLowercase"
  },
  {
    "key": "ctrl+k ctrl+shift+r",
    "command": "workbench.action.reloadWindow"
  },
  {
    "key": "ctrl+shift+oem_2",
    "command": "editor.action.joinLines"
  },
  {
    "key": "ctrl+alt+t",
    "command": "openInTerminal"
  }
]
